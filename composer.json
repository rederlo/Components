{
  "name": "rederlo/cake-auth-jwt",
  "description": "CakePHP plugin for authenticating using JSON Web Tokens and others features",
  "type": "library",
  "version" : "0.0.1",
  "minimum-stability" : "dev",
  "keywords": [
    "cakephp",
    "authenticate",
    "authentication",
    "jwt"
  ],
  "homepage": "https://github.com/rederlo",
  "authors": [
    {
      "name":"rederlo",
      "role":"Author",
      "homepage":"https://github.com/rederlo"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "cakephp/cakephp": "3.3.*",
    "cakephp/plugin-installer": "*",
    "firebase/php-jwt": "~4.0",
    "admad/cakephp-jwt-auth": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "<6.0"
  },
  "autoload": {
    "psr-4": {
      "AuthJwt\\Auth\\" : "src/Auth",
      "Routing\\Filter\\" : "src/Auth"
    }
  },
  "autoload-dev": {
    "psr-4": {

    }
  }
}
