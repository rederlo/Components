{
  "name": "rederlo/cake-auth-jwt",
  "type": "cakephp-plugin",
  "description": "CakePHP plugin for authenticating using JSON Web Tokens and others features",
  "keywords": [
    "cakephp",
    "authenticate",
    "authentication",
    "jwt"
  ],
  "homepage": "https://github.com/rederlo",
  "authors": [
    {
      "name":"rederlo",
      "role":"Author",
      "homepage":"https://github.com/rederlo"
    }
  ],
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "3.3.*",
    "firebase/php-jwt": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "<6.0"
  },
  "autoload": {
    "psr-4": {
      "Personaliza\\Auth\\" : "src/Auth",
      "Personaliza\\Entity\\" : "src/Entity",
      "Personaliza\\Controller\\" : "src/Controller"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Personaliza\\Auth\\Test\\" : "test/Auth",
      "Personaliza\\Auth\\Entity\\" : "test/Auth",
      "Personaliza\\Auth\\Controller\\" : "test/Auth"
    }
  }
}